{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Warning",
                "Microsoft.EntityFrameworkCore": "Warning",
                "Clinics.Api": "Information",
                "Clinics.Api.Services": "Information"
            }
        },
        "WriteTo": [],
        "Enrich": [
            "FromLogContext",
            "WithMachineName"
        ]
    },
    "LogPaths": {
        "Main": "../../logs/production-.log",
        "Business": "../../logs/business-production-.log"
    }
}